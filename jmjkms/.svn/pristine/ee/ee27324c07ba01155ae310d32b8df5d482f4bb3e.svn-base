package com.jmjk.pojo;

import java.sql.Timestamp;

public class Suggest {

	public int id;
	public String doctorAdvice;
	public String problem;
	public int doctorId;
	public String doctorName;
	public int userId;
	public String userName;
	public String idNum;
	public String belongSystem;
	public String createTime;
	public Suggest() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Suggest(int id,String doctorAdvice,String problem, int doctorId, String doctorName,  int userId, String userName,
			String idNum,String belongSystem,String createTime) {
		super();
		this.id=id;
		this.doctorAdvice = doctorAdvice;
		this.doctorId = doctorId;
		this.doctorName = doctorName;
		this.userId = userId;
		this.userName = userName;
		this.idNum = idNum;
		this.belongSystem=belongSystem;
		this.problem=problem;
		this.createTime=createTime;
	}


	public String getProblem() {
		return problem;
	}

	public void setProblem(String problem) {
		this.problem = problem;
	}


	public int getId() {
		return id;
	}

	public String getCreateTime() {
		return createTime;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getBelongSystem() {
		return belongSystem;
	}

	public void setBelongSystem(String belongSystem) {
		this.belongSystem = belongSystem;
	}

	@Override
	public String toString() {
		return "Suggest [doctorAdvice=" + doctorAdvice + ", doctorId=" + doctorId + ", doctorName=" + doctorName
				+ ", userId=" + userId + ", userName=" + userName + ", idNum=" + idNum + "]";
	}

	public String getDoctorAdvice() {
		return doctorAdvice;
	}

	public void setDoctorAdvice(String doctorAdvice) {
		this.doctorAdvice = doctorAdvice;
	}

	public int getDoctorId() {
		return doctorId;
	}

	public void setDoctorId(int doctorId) {
		this.doctorId = doctorId;
	}

	public String getDoctorName() {
		return doctorName;
	}

	public void setDoctorName(String doctorName) {
		this.doctorName = doctorName;
	}


	
	


	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getIdNum() {
		return idNum;
	}

	public int getUserId() {
		return userId;
	}

	public void setUserId(int userId) {
		this.userId = userId;
	}

	public void setIdNum(String idNum) {
		this.idNum = idNum;
	}

}
